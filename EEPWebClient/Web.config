	<system.web>
		<anonymousIdentification enabled="true"/>
		<profile defaultProvider="SqlProvider">
			<providers>
				<add name="SqlProvider" type="System.Web.Profile.SqlProfileProvider" connectionStringName="SqlServices" applicationName="/"/>
			</providers>
			<properties>
				<add name="TestTable" allowAnonymous="true"/>
			</properties>
		</profile>
